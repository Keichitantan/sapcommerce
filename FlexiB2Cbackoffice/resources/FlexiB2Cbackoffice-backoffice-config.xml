	<context component="create-wizard" type="Plantilla">
		<wz:flow xmlns:wz="http://www.hybris.com/cockpitng/config/wizard-config" id="PlantillaWizard" title="Crear Plantilla">
			<wz:prepare id="plantillaPrepare">
				<wz:initialize property="newPlantilla" type="Plantilla"/>
			</wz:prepare>
			<wz:step id="step1" label="Plantilla">
				<wz:content id="step1.content">
					<wz:property-list root="newPlantilla">
						<wz:property qualifier="id"/>
						<wz:property qualifier="name"/>
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:done >
						<wz:save property="newPlantilla"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>

