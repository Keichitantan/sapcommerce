        },
        {
            "key": "azure.hotfolder.storage.account.name",
            "value": "mgipk3bjvi4ioir3hcivafy"
        },
        {
            "key": "cloud.hotfolder.storage.file.sort.name.prefix.priority",
            "value": "coredata,sampledata,product,url_media"
        },
        {
            "key": "cloud.hotfolder-storage.file.sort.name.sequence",
